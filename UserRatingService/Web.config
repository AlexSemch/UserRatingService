<?xml version="1.0" encoding="utf-8"?>
<configuration>
 <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5.1" />
    <httpRuntime targetFramework="4.5.1" />
  </system.web>
  <system.serviceModel>
    <services>
      <service name ="UserRateService">
        <endpoint address="net.tcp://localhost:8000/UserRateService" binding="netTcpBinding" 
                   contract="UserRatingService.IUserRateService" />
      </service>
    </services>
  </system.serviceModel>
</configuration>